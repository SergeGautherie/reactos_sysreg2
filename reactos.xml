<!--
AppVeyor, 'kvm':
libvirt: QEMU Driver error : internal error: QEMU / QMP failed: Could not access KVM kernel module: No such file or directory
failed to initialize KVM: No such file or directory
-->
<domain type='qemu'>
  <name>ReactOS_AppVeyor-QEMU</name>
  <uuid>0ab69219-58c2-1e5c-6beb-fb29db74b0af</uuid>
  <!--  524288 = 512 MiB -->
  <!--  655360 = 640 MiB -->
  <!--  786432 = 768 MiB -->
  <!-- 1048576 =   1 GiB -->
  <memory>786432</memory>
  <currentMemory>786432</currentMemory>
  <vcpu>1</vcpu>
  <os>
    <type arch='i686' machine='pc'>hvm</type>
    <boot dev='cdrom'/>
  </os>
  <features>
    <!-- <acpi/> -->
  </features>
  <clock offset='localtime'/>
  <on_poweroff>destroy</on_poweroff>
  <on_reboot>destroy</on_reboot>
  <on_crash>destroy</on_crash>
  <devices>
    <emulator>/usr/bin/qemu-system-i386</emulator>
    <disk type='file' device='disk'>
      <source file='/sysreg2_workdir/TestRun/ros-sysreg.img'/>
      <target dev='hda' bus='ide'/>
    </disk>
    <disk type='file' device='cdrom'>
      <source file='/sysreg2_workdir/TestRun/bootcdregtest.iso'/>
      <target dev='hdc' bus='ide'/>
      <readonly/>
    </disk>
    <interface type='user'>
      <mac address='00:16:3e:2e:dd:de'/>
      <model type='pcnet'/>
    </interface>
<!--    <serial type='pty'>
      <source path='/dev/pts/4'/>
      <target port='1'/>
    </serial> -->
    <console type='pty' tty='/dev/pts/4'>
      <source path='/dev/pts/4'/>
      <target port='1'/>
    </console>

    <!-- Disabled as of 2011-10-16, ask Amine or Cameron for details
    <input type='tablet' bus='usb'/-->

    <input type='mouse' bus='ps2'/>
    <graphics type='vnc' autoport='no' port='5900' listen='127.0.0.1' passwd='PASSWORD_HERE' />
  </devices>
</domain>
